{
	"metadata": [
		{
			"src": [
				{
					"files": [
						"General/RestClient/RestClient/RestClient.csproj",
						"RestPki/RestPkiClient/RestPkiClient/Lacuna.RestPki.Client.csproj"
					],
					"exclude": [
						"**/bin/**",
						"**/obj/**"
					],
					"cwd": "../../.."
				}
			],
			"dest": "../api",
			"properties": {
				"TargetFramework": "net45"
			}
		}
	],
	"build": {
		"content": [
			{
				"cwd": "..",
				"files": [
					"api/**.yml",
					"api/index.md"
				]
			},
			{
				"files": [
					"articles/**.md",
					"articles/**/toc.yml",
					"toc.yml",
					"*.md"
				],
				"exclude": [
					"obj/**",
					"_site/**"
				]
			}
		],
		"resource": [
			{
				"cwd": "..",
				"files": [
					"images/**",
					"content/**"
				],
				"exclude": [
					"obj/**",
					"_site/**"
				]
			}
		],
		"overwrite": [
			{
				"files": [ "apidoc/**.md" ],
				"exclude": [
					"obj/**",
					"_site/**"
				]
			}
		],
		"dest": "../_site/en-us",
		"globalMetadataFiles": [],
		"fileMetadataFiles": [],
		"template": [ "default" ],
		"postProcessors": [],
		"noLangKeyword": false,
		"xref": [ "../_zip/msdn.4.5.2.zip" ]
	}
}